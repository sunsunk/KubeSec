{
  "variables": [
    {
      "name": "start_time",
      "type": "PX_STRING",
      "description": "The relative start time of the window. Current time is assumed to be now",
      "defaultValue": "-5m"
    },
    {
      "name": "pod",
      "type": "PX_POD",
      "description": "The full/partial name of the pod to filter by for SQL request. Format: ns/pod_name",
      "defaultValue": ""
    },
    {
      "name": "query",
      "type": "PX_STRING",
      "description": "The normalzied SQL query. Eg. SELECT * FROM foo WHERE bar=$1"
    }
  ],
  "globalFuncs": [
    {
      "outputName": "LET",
      "func": {
        "name": "pod_sql_let",
        "args": [
          {
            "name": "start_time",
            "variable": "start_time"
          },
          {
            "name": "pod",
            "variable": "pod"
          },
          {
            "name": "normed_query",
            "variable": "query"
          }
        ]
      }
    }
  ],
  "widgets": [
    {
      "name": "P50 Latency",
      "position": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 3
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "latency_p50",
            "series": "params",
            "stackBySeries": false,
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "P50 Latency"
        },
        "xAxis": null
      }
    },
    {
      "name": "P90 Latency",
      "position": {
