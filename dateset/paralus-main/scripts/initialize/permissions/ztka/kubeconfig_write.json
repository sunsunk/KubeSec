  "resource_action_urls": [
    {
      "url": "/revoke",
      "methods": [
        "POST"
      ]
    },
    {
      "url": "/user/:account_id/revoke",
      "methods": [
        "POST"
      ]
    },
    {
      "url": "/ssouser/:account_id/revoke",
      "methods": [
        "POST"
      ]
    },
    {
      "url": "/user/:account_id/setting",
      "methods": [
        "PUT"
      ]
    },
    {
      "url": "/ssouser/:account_id/setting",
      "methods": [
        "PUT"
      ]
    },
    {
      "url": "/organization/:organization_id/setting",
      "methods": [
        "PUT"
      ]
    }
  ],
  "authenticated": true,
  "scope": "PROJECT"
}