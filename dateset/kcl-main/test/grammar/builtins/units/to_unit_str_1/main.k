import units

# SI
K = int(units.to_K(1000))
M = int(units.to_M(1000000))
G = int(units.to_G(1000000000))
T = int(units.to_T(1000000000000))
P = int(units.to_P(1000000000000000))
# IEC
Ki = int(units.to_Ki(1024))
Mi = int(units.to_Mi(1024 ** 2))
Gi = int(units.to_Gi(1024 ** 3))
Ti = int(units.to_Ti(1024 ** 4))
Pi = int(units.to_Pi(1024 ** 5))
