{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Custom",
  "description": "An empty schema for custom document entries",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "Id",
      "description": "Unique id"
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "title": "Name",
      "description": "Name of entity shown in UI"
    }
  },
  "additionalProperties": true,
  "required": [
    "id",
    "name"
  ]
}
