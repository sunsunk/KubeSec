{
    "compilerOptions": {
      "target": "es6",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "ESNext",
      "moduleResolution": "node",
      "noFallthroughCasesInSwitch": true,
      "outDir": "electron/",
      "noEmit": true
    },
    "include": [
      "electron/**/*"
    ]
  }
  